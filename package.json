{
  "name": "iawp-org",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "sosout <sosout@139.com>",
  "private": true,
  "scripts": {
    "dev": "node scripts/server.js",
    "start": "nuxt start -p $PORT",
    "local-build": "nuxt build",
    "dev-build": "nuxt build && PORT=3003 npm start --api=dev",
    "pre-build": "nuxt build && PORT=3003 npm start --api=pre",
    "prod-build": "nuxt build && PORT=3003 npm start --api=prod",
    "analyze": "nuxt build --analyze",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "element-ui": "^2.4.1",
    "nuxt": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0"
  }
}
