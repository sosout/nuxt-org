{
  "name": "nuxt-org",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "sosout <sosout@139.com>",
  "private": true,
  "scripts": {
    "dev": "node scripts/server.js",
    "hook-dev": "nuxt",
    "start": "PORT=3003 nuxt start",
    "local-build": "nuxt build --api=dev",
    "dev-build": "nuxt build && npm run start --api=dev",
    "pre-build": "nuxt build && npm run start --api=pre",
    "prod-build": "nuxt build && npm run start --api=prod",
    "analyze": "nuxt build --analyze",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "config": {
    "nuxt": {
      "host": "127.0.0.1",
      "port": "8888"
    }
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.0.8",
    "big.js": "^5.1.2",
    "element-ui": "^2.4.1",
    "nuxt": "^1.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0"
  }
}
